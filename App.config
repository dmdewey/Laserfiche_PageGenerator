<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
  <appSettings>
    <add key="LaserficheServer" value="laserfiche.example.com"/>
    <add key="LaserficheRepository" value="Laserfiche-Repository"/>
    <add key="LaserficheUser" value="user"/>
    <add key="LaserfichePassword" value="password"/>
    <add key="TemporarySaveFolder" value="c:\\temp\\"/>
    <add key="TiffImageType" value="tiff24nc"/>
    <add key="TiffImageResolution" value="300"/>
  </appSettings>
</configuration>
